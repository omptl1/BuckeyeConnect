

<div id="<%= dom_id user %>">

  <p>
    <strong>Role:</strong>
    <%= user.role %>
  <p>
    <strong>First Name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last Name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>
   <p>
    <strong>Password Confirmation:</strong>
    <%= user.password_confirmation %>
  </p>



</div>
