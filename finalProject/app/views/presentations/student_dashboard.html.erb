Rails.logger.debug("Session Data: #{session.inspect}")


<h1>My Presentations</h1>

<div id="presentations">
  <% @presentations.each do |presentation| %>
    <%= render presentation %>
    <p>
      <%= link_to "Show this presentation", presentation %>
    </p>
  <% end %>
  
</div>

